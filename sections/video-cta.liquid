

{% schema %}
{
    "name": {
        "en": "Video CTA"
    },
    "settings": [
        {
            "type": "text",
            "id": "vc_title",
            "label": {
                "en": "Title"
            },
            "default": "Spring Girls"
        },
        {
            "type": "textarea",
            "id": "content",
            "label": {
                "en": "Description"
            },
            "default": "Girl Lookbook 2015"
        },
        {
            "type": "url",
            "id": "link",
            "label": {
                "en": "Link"
            }
        },
        {
            "type": "image_picker",
            "id": "image",
            "label": {
                "en": "Lookbook Cover"
            }
        },
        {
            "type": "video_url",
            "id": "video_url",
            "label": {
                "en": "Video URL"
            }
        },
        {
            "type": "text",
            "id": "video_title",
            "label": {
                "en": "Video Title"
            }
        },
        {
            "type": "textarea",
            "id": "video_desc",
            "label": {
                "en": "Video Description"
            }
